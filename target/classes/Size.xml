<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Size">
        <table name="size" schema="assignment_java4" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="sizeName">
                <column name="size_name"/>
            </basic>
            <one-to-many name="productDetailsById" mapped-by="sizeBySizeId" target-entity="entity.ProductDetails"/>
        </attributes>
    </entity>
</entity-mappings>
